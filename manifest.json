{
    "name": "RedSearch",
    "version": "0.2",
    "description": "Searching RED vela tasks or tables.",
    "background": {
      "scripts": ["js/background.js"],
      "persistent": false
    },
    "manifest_version": 2,
    "permissions": ["contextMenus","tabs"],
    "icons":{
            "16": "img/icon.png",
            "48": "img/icon.png",
            "128": "img/icon.png"
    },
    "browser_action":
    {
        "default_title": "用于搜索vela任务及表格信息"
    }
  }